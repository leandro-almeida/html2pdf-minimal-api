		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<RootNamespace>HtmlToPdfApi</RootNamespace>
		<UserSecretsId>5479b430-1d17-4c74-ba46-7e8895910f4d</UserSecretsId>
